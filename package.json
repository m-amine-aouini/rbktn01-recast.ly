    "babel-plugin-react": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "envify": "^4.1.0",
    "mocha": "2.3.3",
    "sinon": "1.17.3",
    "terser": "^4.6.6",
    "terser-brunch": "^4.0.0",
    "uglifyify": "^5.0.2"
